{
  "name": "simplegtd.com",
  "version": "0.13.6",
  "main": "index.js",
  "license": "MIT",
  "engines": {
    "node": "v7.10.0"
  },
  "private": true,
  "repository": {
    "url": "https://github.com/jigargosar/elm-simple-gtd"
  },
  "scripts": {
    "install-elm": "which -a elm ; elm-package install -y",
    "_postinstall": "which -a elm && elm-package install -y && bower install",
    "postinstall": "yarn global add elm@0.18.0",
    "dev": "cross-env NODE_ENV=development webpack-dev-server",
    "hot": "cross-env NODE_ENV=development webpack-dev-server --hot --inline",
    "hot-hot": "nodemon --watch webpack.config.babel.js --watch package.json --exec \"npm run hot\"",
    "link": "ln -Fs `pwd`/src/web/bower_components static/bower_components; ln -Fs `pwd`/src/web/bower_components dev/bower_components",
    "start": "http-server docs"
  },
  "devDependencies": {
    "babel-core": "6.24.1",
    "babel-loader": "7.0.0",
    "babel-preset-env": "1.5.1",
    "bower": "1.8.0",
    "copy-webpack-plugin": "4.0.1",
    "cross-env": "5.0.0",
    "css-loader": "0.28.2",
    "elm-hot-loader": "0.5.4",
    "elm-webpack-loader": "4.3.1",
    "file-loader": "0.11.1",
    "firebase-tools": "3.9.0",
    "kefir": "3.7.1",
    "nodemon": "1.11.0",
    "polymer-cli": "1.0.2",
    "postcss": "6.0.1",
    "postcss-browser-reporter": "0.5.0",
    "postcss-cssnext": "2.11.0",
    "postcss-import": "10.0.0",
    "postcss-loader": "2.0.5",
    "postcss-reporter": "3.0.0",
    "postcss-url": "6.1.0",
    "release-tools": "2.5.2",
    "rimraf": "2.6.1",
    "runjs": "3.4.1",
    "serviceworker-webpack-plugin": "0.2.2",
    "style-loader": "0.18.0",
    "url-loader": "0.5.8",
    "webpack": "2.6.0",
    "webpack-dev-server": "2.4.5"
  },
  "dependencies": {
    "babel-polyfill": "6.23.0",
    "crypto-random-string": "1.0.0",
    "howler": "2.0.3",
    "jquery": "3.2.1",
    "jquery-ui": "1.12.1",
    "pouchdb-browser": "6.2.0",
    "pouchdb-find": "6.2.0",
