{
  "name": "simplegtd.com",
  "version": "0.12.15",
  "main": "index.js",
  "license": "MIT",
  "engines": {
    "node": "v7.10.0"
  },
  "private": true,
  "repository": {
    "url": "https://github.com/jigargosar/elm-simple-gtd"
  },
  "scripts": {
    "install-elm": "which -a elm ; elm-package install -y",
    "_postinstall": "which -a elm && elm-package install -y && bower install",
    "postinstall": "yarn global add elm@0.18.0 elm-hot-loader@0.5.4 elm-webpack-loader@4.3.1",
    "dev": "cross-env NODE_ENV=development webpack-dev-server",
    "hot": "cross-env NODE_ENV=development webpack-dev-server --hot --inline",
    "hot-hot": "nodemon --watch webpack.config.js --watch package.json --exec \"npm run hot\"",
    "prebuild": "bash -c \"rimraf app && rimraf docs && rimraf build \"",
    "bump": "npm_bump --auto --auto-fallback patch 2>&1 | awk 'BEGIN{s=0} /Error/{s=1} 1; END{exit(s)}'",
    "_bump": "npm_bump --auto --auto-fallback patch --skip-push 2>&1 | awk 'BEGIN{s=0} /Error/{s=1} 1; END{exit(s)}'",
    "_postbump": "npm run build && npm run git:commit-docs && npm run deploy-dev",
    "git:commit-docs": "run build:commitDocs",
    "build": "bash scripts/build.sh",
    "prebuild-dev": "rimraf dev",
    "build-dev": "bash scripts/build-dev.sh",
    "predeploy-dev": "npm run build-dev",
    "deploy-dev": "firebase deploy --except functions --project dev --public dev",
    "link": "ln -Fs `pwd`/src/web/bower_components static/bower_components; ln -Fs `pwd`/src/web/bower_components dev/bower_components",
    "start": "http-server docs"
  },
  "devDependencies": {
    "babel-core": "6.24.1",
    "babel-loader": "7.0.0",
    "babel-preset-env": "1.4.0",
    "bower": "1.8.0",
    "copy-webpack-plugin": "4.0.1",
    "cross-env": "4.0.0",
    "css-loader": "0.28.1",
    "file-loader": "0.11.1",
    "firebase-tools": "3.9.0",
    "kefir": "3.7.1",
    "nodemon": "1.11.0",
    "polymer-cli": "0.18.2",
    "postcss": "5.2.17",
    "postcss-browser-reporter": "0.5.0",
    "postcss-cssnext": "2.10.0",
    "postcss-import": "9.1.0",
    "postcss-loader": "1.3.3",
    "postcss-reporter": "3.0.0",
    "postcss-url": "6.0.4",
    "release-tools": "2.5.2",
    "rimraf": "2.6.1",
    "runjs": "3.4.1",
    "serviceworker-webpack-plugin": "0.2.1",
    "style-loader": "0.17.0",
    "url-loader": "0.5.8",
    "webpack": "2.5.0",
    "webpack-dev-server": "2.4.5"
  },
  "dependencies": {
    "babel-polyfill": "6.23.0",
    "crypto-random-string": "1.0.0",
    "howler": "2.0.3",
    "jquery": "3.2.1",
    "jquery-ui": "1.12.1",
    "pouchdb-browser": "6.2.0",
    "pouchdb-find": "6.2.0",
    "pouchdb-upsert": "2.2.0",
    "ramda": "0.23.0",
    "tabtrap": "1.2.6"
  }
}
