{
  "rules": {
    "users": {
      "$uid": {
        ".read": "$uid === auth.uid",
        ".write": "$uid === auth.uid"
      }
    },
    "notifications": {
      ".indexOn": [
        "time"
      ],
      "$uidTodoId":{
        ".write":"newData.child('uid').val() === auth.uid && $uidTodoId === auth.uid + '---' + newData.child('todoId').val() && newData.child('timestamp').val().isNumber()"
      }
    }
  }
}
