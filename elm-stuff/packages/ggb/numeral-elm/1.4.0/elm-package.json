{
    "version": "1.4.0",
    "summary": "Elm module for (advanced) number formatting. Numeral.js port to Elm",
    "repository": "https://github.com/ggb/numeral-elm.git",
    "license": "MIT",
    "source-directories": [
        "src/"
    ],
    "exposed-modules": [
        "Numeral",
        "Language",
        "Languages.English",
        "Languages.BritishEnglish",
        "Languages.German",
        "Languages.French",
        "Languages.Japanese",
        "Languages.Italian",
        "Languages.Russian",
        "Languages.Ukrainian"
    ],
    "dependencies": {
        "elm-lang/core": "5.0.0 <= v < 6.0.0"
    },
    "elm-version": "0.18.0 <= v < 0.19.0"
}
