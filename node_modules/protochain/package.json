{
  "name": "protochain",
  "version": "1.0.5",
  "description": "Get the prototype chain of any value as an Array",
  "main": "protochain.js",
  "scripts": {
    "test": "(babel test.js > protochain-test.js) && tape protochain-test.js && standard index.js test.js",
    "prepublish": "babel index.js > protochain.js",
    "pretest": "npm run prepublish",
    "posttest": "rm protochain-test.js"
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "keywords": [
    "object",
    "inherit",
    "inheritance",
    "prototypical",
    "prototype",
    "utility",
    "proto",
    "hierarchy",
    "ancestors",
    "grandparents",
    "parents"
  ],
  "author": "Tim Oxley <secoif@gmail.com>",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.10.1",
    "babel-preset-es2015": "^6.9.0",
    "standard": "^7.1.2",
    "tape": "^4.6.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/timoxley/protochain.git"
  },
  "bugs": {
    "url": "https://github.com/timoxley/protochain/issues"
  },
  "homepage": "https://github.com/timoxley/protochain"
}
